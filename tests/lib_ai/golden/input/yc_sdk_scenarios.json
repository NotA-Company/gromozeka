[
  {
    "name": "YcSDK Text",
    "description": "Text generation with YC SDK provider",
    "module": "tests.lib_ai.model_wrappers",
    "class": "YcSdkModelWrapper",
    "method": "generateText",
    "init_kwargs": {
      "providerConfig": {
        "folder_id": "${YC_FOLDER_ID}",
        "yc_profile": "${YC_PROFILE}"
      },
      "modelArgs": {
        "model_id": "yandexgpt-lite",
        "model_version": "rc",
        "temperature": 0.5,
        "context_size": 32768,
        "support_tools": false,
        "support_text": true,
        "support_images": false,
        "can_be_choosen": true
      }
    },
    "kwargs": {
      "messages": [
        {
          "module": "lib.ai.models",
          "class": "ModelMessage",
          "init_kwargs": {
            "role": "user",
            "content": "Hello, how are you?"
          }
        }
      ]
    }
  },
  {
    "name": "ycSDK Image",
    "description": "Image generation with YC SDK provider",
    "module": "tests.lib_ai.model_wrappers",
    "class": "YcSdkModelWrapper",
    "method": "generateImage",
    "init_kwargs": {
      "providerConfig": {
        "api_key": "${YC_API_KEY}",
        "folder_id": "${YC_FOLDER_ID}"
      },
      "modelArgs": {
        "model_id": "yandex-art",
        "model_version": "latest",
        "temperature": 1,
        "context_size": 32768,
        "support_tools": false,
        "support_text": false,
        "support_images": true
      }
    },
    "kwargs": {
      "messages": [
        {
          "module": "lib.ai.models",
          "class": "ModelMessage",
          "init_kwargs": {
            "role": "user",
            "content": "Create an image of a beautiful sunset."
          }
        }
      ]
    }
  }
]