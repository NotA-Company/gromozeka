[
  {
    "name": "yc_sdk_text_generation",
    "description": "Text generation with YC SDK provider",
    "module": "tests.lib_ai.model_wrappers",
    "class": "YcSdkModelWrapper",
    "method": "generateText",
    "init_kwargs": {
      "providerConfig": {
        "folder_id": "${YC_FOLDER_ID}",
        "yc_profile": "${YC_PROFILE}"
      },
      "modelArgs": {
        "model_id": "yandexgpt",
        "model_version": "latest",
        "temperature": 0.7,
        "context_size": 8192
      }
    },
    "kwargs": {
      "messages": [
        {
          "module": "lib.ai.models",
          "class": "ModelMessage",
          "init_kwargs": {
            "role": "user",
            "content": "Hello, how are you?"
          }
        }
      ]
    }
  },
