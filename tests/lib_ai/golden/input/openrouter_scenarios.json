[
  {
    "name": "OpenRouter Basic",
    "description": "Basic text generation with OpenRouter provider",
    "module": "tests.lib_ai.model_wrappers",
    "class": "OpenrouterModelWrapper",
    "method": "generateText",
    "init_kwargs": {
      "providerConfig": {
        "api_key": "${OPENROUTER_API_KEY}"
      },
      "modelArgs": {
        "model_id": "meta-llama/llama-3.2-1b-instruct",
        "model_version": "latest",
        "temperature": 0.3,
        "context_size": 64000,
        "support_tools": true,
        "support_text": true,
        "support_images": false
      }
    },
    "kwargs": {
      "messages": [
        {
          "module": "lib.ai.models",
          "class": "ModelMessage",
          "init_kwargs": {
            "role": "user",
            "content": "Hello, how are you?"
          }
        }
      ]
    }
  }
]