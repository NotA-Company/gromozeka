[
  {
    "name": "openrouter_basic",
    "description": "Basic text generation with OpenRouter provider",
    "module": "tests.lib_ai.model_wrappers",
    "class": "OpenrouterModelWrapper",
    "method": "generateText",
    "init_kwargs": {
      "providerConfig": {
        "api_key": "${OPENROUTER_API_KEY}"
      },
      "modelArgs": {
        "model_id": "mistralai/mistral-7b-instruct",
        "model_version": "latest",
        "temperature": 0.7,
        "context_size": 32768
      }
    },
    "kwargs": {
      "messages": [
        {
          "module": "lib.ai.models",
          "class": "ModelMessage",
          "init_kwargs": {
            "role": "user",
            "content": "Hello, how are you?"
          }
        }
      ]
    }
  },
